{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/", "build/"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": []
    }
  }
}
